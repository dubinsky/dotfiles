# DevPod Start cloud-run.devpod
Host cloud-run.devpod
  ForwardAgent yes
  LogLevel error
  StrictHostKeyChecking no
  UserKnownHostsFile /dev/null
  HostKeyAlgorithms rsa-sha2-256,rsa-sha2-512,ssh-rsa
  ProxyCommand "/usr/bin/devpod-cli" ssh --stdio --context default --user vscode cloud-run
  User vscode
# DevPod End cloud-run.devpod
Host *
  ControlMaster		auto
  ControlPersist	1m
  # control directory needs to be created manually
  ControlPath		~/.ssh/control/ssh-%r@%h:%p